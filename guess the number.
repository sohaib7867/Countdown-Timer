import random

number = random.randint(0, 99)
  
while True:
    user_input = int(input("Enter a number: "))
    if user_input > number:
        print("Your guess is too high")
    elif user_input < number:
        print("Your guess is too low")
    else:
        print("Congrats! The number was: ", number)
        break     
